<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="definitions_loan_rules" name="LoanRules" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.25.0">
  <decision id="loanApprovalDecision" name="Loan Approval Decision">
    <decisionTable id="decisionTable_loan" hitPolicy="FIRST">
      <input id="input_blacklist" label="Blacklist check">
        <inputExpression id="inputExpression_blacklist" typeRef="boolean">
          <text>blackList</text>
        </inputExpression>
      </input>
      <input id="input_scoring" label="Scoring value">
        <inputExpression id="inputExpression_scoring" typeRef="integer">
          <text>scoring</text>
        </inputExpression>
      </input>
      <output id="output_approval" label="Approval result" typeRef="string" />
      <rule id="rule1">
        <inputEntry id="inputEntry1_blacklist">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry1_scoring">
          <text>-</text>
        </inputEntry>
        <outputEntry id="outputEntry1">
          <text>"REJECTED_BLACKLIST"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0tdsvbn">
        <inputEntry id="UnaryTests_1as1l6p">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_12wqc2t">
          <text>777</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_12zuzgf">
          <text>"LUCKY"</text>
        </outputEntry>
      </rule>
      <rule id="rule2">
        <inputEntry id="inputEntry2_blacklist">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry2_scoring">
          <text>&lt; 50</text>
        </inputEntry>
        <outputEntry id="outputEntry2">
          <text>"REJECTED_SCORING"</text>
        </outputEntry>
      </rule>
      <rule id="rule3">
        <inputEntry id="inputEntry3_blacklist">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry3_scoring">
          <text>&gt;=50</text>
        </inputEntry>
        <outputEntry id="outputEntry3">
          <text>"APPROVED"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_147kcbn">
        <inputEntry id="UnaryTests_15h0esr">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1au5e5y">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_07pgnm1">
          <text>"UNDEFINED_SCORING"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
